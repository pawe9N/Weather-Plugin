{
  "manifest_version": 2,

  "name": "Weather plugin",
  "description": "Weather Plugin",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
  	"64": "icon.png"
  },
  "content_scripts":[
  	{
  		"matches": ["http://*/*", "https://*/", "<all_urls>"],
  		"js": ["script.js", "jquery-3.2.1.js"]
  	}
  ]
}
